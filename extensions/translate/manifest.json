{
	"name": "Translate",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "A translate plugin for chrome",
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"css": ["translate.css"],
			"js": ["translate.js"],
			"run_at": "document_idle"
		}
	],
	"permissions": [
		"tabs", "http://*/"
	]
}